{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "projects": {
        "core": {
            "projectType": "application",
            "schematics": {
                "@nrwl/angular:component": {
                    "style": "scss"
                }
            },
            "root": "apps/core",
            "sourceRoot": "apps/core/src",
            "prefix": "cyberse",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/apps/core",
                        "index": "apps/core/src/index.html",
                        "main": "apps/core/src/main.ts",
                        "polyfills": "apps/core/src/polyfills.ts",
                        "tsConfig": "apps/core/tsconfig.app.json",
                        "aot": true,
                        "assets": [
                            "apps/core/src/favicon.ico",
                            "apps/core/src/assets"
                        ],
                        "styles": ["apps/core/src/styles.scss"],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/core/src/environments/environment.ts",
                                    "with": "apps/core/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "core:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "core:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "core:build"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "apps/core/tsconfig.app.json",
                            "apps/core/tsconfig.spec.json"
                        ],
                        "exclude": ["**/node_modules/**", "!apps/core/**"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "apps/core/jest.config.js",
                        "tsConfig": "apps/core/tsconfig.spec.json",
                        "setupFile": "apps/core/src/test-setup.ts"
                    }
                }
            }
        },
        "core-e2e": {
            "root": "apps/core-e2e",
            "sourceRoot": "apps/core-e2e/src",
            "projectType": "application",
            "architect": {
                "e2e": {
                    "builder": "@nrwl/cypress:cypress",
                    "options": {
                        "cypressConfig": "apps/core-e2e/cypress.json",
                        "tsConfig": "apps/core-e2e/tsconfig.e2e.json",
                        "devServerTarget": "core:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "core:serve:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": ["apps/core-e2e/tsconfig.e2e.json"],
                        "exclude": ["**/node_modules/**", "!apps/core-e2e/**"]
                    }
                }
            }
        },
        "search-bar": {
            "projectType": "library",
            "root": "libs/search-bar",
            "sourceRoot": "libs/search-bar/src",
            "prefix": "cyberse",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-ng-packagr:build",
                    "options": {
                        "tsConfig": "libs/search-bar/tsconfig.lib.json",
                        "project": "libs/search-bar/ng-package.json"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "libs/search-bar/tsconfig.lib.json",
                            "libs/search-bar/tsconfig.spec.json"
                        ],
                        "exclude": ["**/node_modules/**", "!libs/search-bar/**"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "libs/search-bar/jest.config.js",
                        "tsConfig": "libs/search-bar/tsconfig.spec.json",
                        "setupFile": "libs/search-bar/src/test-setup.ts"
                    }
                }
            },
            "schematics": {
                "@nrwl/angular:component": {
                    "styleext": "scss"
                }
            }
        },
        "hangman-api": {
            "root": "apps/hangman-api",
            "sourceRoot": "apps/hangman-api/src",
            "projectType": "application",
            "prefix": "hangman-api",
            "schematics": {},
            "architect": {
                "build": {
                    "builder": "@nrwl/node:build",
                    "options": {
                        "outputPath": "dist/apps/hangman-api",
                        "main": "apps/hangman-api/src/main.ts",
                        "tsConfig": "apps/hangman-api/tsconfig.app.json"
                    },
                    "configurations": {
                        "production": {
                            "optimization": true,
                            "extractLicenses": true,
                            "inspect": false,
                            "fileReplacements": [
                                {
                                    "replace": "apps/hangman-api/src/environments/environment.ts",
                                    "with": "apps/hangman-api/src/environments/environment.prod.ts"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@nrwl/node:execute",
                    "options": {
                        "buildTarget": "hangman-api:build"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "apps/hangman-api/tsconfig.app.json",
                            "apps/hangman-api/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**",
                            "!apps/hangman-api/**"
                        ]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "apps/hangman-api/jest.config.js",
                        "tsConfig": "apps/hangman-api/tsconfig.spec.json"
                    }
                }
            }
        },
        "hangman": {
            "projectType": "application",
            "schematics": {
                "@nrwl/angular:component": {
                    "style": "scss"
                }
            },
            "root": "apps/hangman",
            "sourceRoot": "apps/hangman/src",
            "prefix": "cyberse",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/apps/hangman",
                        "index": "apps/hangman/src/index.html",
                        "main": "apps/hangman/src/main.ts",
                        "polyfills": "apps/hangman/src/polyfills.ts",
                        "tsConfig": "apps/hangman/tsconfig.app.json",
                        "aot": true,
                        "assets": [
                            "apps/hangman/src/favicon.ico",
                            "apps/hangman/src/assets"
                        ],
                        "styles": ["apps/hangman/src/styles.scss"],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/hangman/src/environments/environment.ts",
                                    "with": "apps/hangman/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "hangman:build",
                        "proxyConfig": "apps/hangman/proxy.conf.json"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "hangman:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "hangman:build"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "apps/hangman/tsconfig.app.json",
                            "apps/hangman/tsconfig.spec.json"
                        ],
                        "exclude": ["**/node_modules/**", "!apps/hangman/**"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "apps/hangman/jest.config.js",
                        "tsConfig": "apps/hangman/tsconfig.spec.json",
                        "setupFile": "apps/hangman/src/test-setup.ts"
                    }
                }
            }
        },
        "hangman-e2e": {
            "root": "apps/hangman-e2e",
            "sourceRoot": "apps/hangman-e2e/src",
            "projectType": "application",
            "architect": {
                "e2e": {
                    "builder": "@nrwl/cypress:cypress",
                    "options": {
                        "cypressConfig": "apps/hangman-e2e/cypress.json",
                        "tsConfig": "apps/hangman-e2e/tsconfig.e2e.json",
                        "devServerTarget": "hangman:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "hangman:serve:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": ["apps/hangman-e2e/tsconfig.e2e.json"],
                        "exclude": [
                            "**/node_modules/**",
                            "!apps/hangman-e2e/**"
                        ]
                    }
                }
            }
        },
        "hangman-models": {
            "root": "libs/hangman-models",
            "sourceRoot": "libs/hangman-models/src",
            "projectType": "library",
            "schematics": {},
            "architect": {
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": ["libs/hangman-models/tsconfig.lib.json"],
                        "exclude": [
                            "**/node_modules/**",
                            "!libs/hangman-models/**"
                        ]
                    }
                }
            }
        }
    },
    "cli": {
        "defaultCollection": "@nrwl/angular"
    },
    "schematics": {
        "@nrwl/angular:application": {
            "unitTestRunner": "jest",
            "e2eTestRunner": "cypress"
        },
        "@nrwl/angular:library": {
            "unitTestRunner": "jest"
        }
    },
    "defaultProject": "core"
}
